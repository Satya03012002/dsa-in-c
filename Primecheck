#include<stdio.h>
int main()
{
    int i,j,k,count=0;
    printf("enter a no.\n");
    scanf("%d",&i);
    for(j=1;j<=i;j++)
    {
        if((i%j)==0)
        {
            count++;
        }
     
        
    }
    if(count == 2)
    {
        printf("prime");
    }
    else
    {
        printf("not prime");
    }
    return 0;
}
